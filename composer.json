{
  "name": "wp-packagist/wp-cli-gdrive-command",
  "description": "Google Drive Cloud Storage for WP-CLI",
  "type": "wp-cli-package",
  "homepage": "https://github.com/wp-packagist/wp-cli-gdrive-command",
  "license": "MIT",
  "keywords": [
    "cli",
    "wordpress",
    "wp-cli"
  ],
  "authors": [
    {
      "name": "Mehrshad Darzi",
      "email": "mehrshad198@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "WP_CLI_GOOGLE_DRIVE\\": "src/"
    },
    "files": [
      "command.php"
    ]
  },
  "require": {
    "php": "^5.5 || ^7.0",
    "wp-packagist/wp-cli-helper": "dev-master",
    "ext-json": "*",
    "ext-curl": "*"
  },
  "extra": {
    "commands": [
      "gdrive",
      "gdrive auth",
      "gdrive ls",
      "gdrive rm",
      "gdrive share",
      "gdrive private",
      "gdrive rename",
      "gdrive copy",
      "gdrive move",
      "gdrive trash",
      "gdrive mkdir",
      "gdrive restore",
      "gdrive get",
      "gdrive upload",
      "gdrive storage"
    ]
  },
  "require-dev": {
    "wp-cli/wp-cli": "^2.0",
    "phpunit/phpunit": "^4.8"
  },
  "scripts": {
    "test": "phpunit"
  },
  "support": {
    "issues": "https://github.com/wp-packagist/wp-cli-gdrive-command/issues"
  }
}
